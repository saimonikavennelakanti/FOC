INPUT:

#include<stdio.h>
int main()
{
int a = 10;
int b = 20;
int c = 30;
printf("Before swapping: a = %d, b = %d, c = %d\n", a, b, c);
int temp = a;
a = b;
b = c;
c = temp;
printf("After swapping: a = %d, b = %d, c = %d\n", a, b, c);
return 0;
}

OUTPUT:

Before swapping: a = 10, b = 20, c = 30
After swapping: a = 20, b = 30, c = 10
